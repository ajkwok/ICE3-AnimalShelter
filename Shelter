public class Shelter
   private String type;
   private double cost;
   
   public Shelter(String species, double cost)
   {
      type = species;
      this.cost = cost;
   }
   public Shelter()
   {
      type = "";
      cost = 0.0;
   }
   public void setType(String species)
   {
      type = species;
   }
   public void setCost(double cost)
   {
      this.cost = cost;
   }
   
}
